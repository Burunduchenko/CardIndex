{
  "Jwt": {
    "Issuer": "http://localhost:44321",
    "Secret": "MySuperPuperUltraMegaSecretKey",
    "ExpirationInDays": 30
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "AdministrationDB": "Server=(localdb)\\mssqllocaldb;Database=AdministrationDB;Trusted_Connection=True;",
    "CardDB": "Server=(localdb)\\mssqllocaldb;Database=CardDB;Trusted_Connection=True;"
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs\\logs_.txt",
          "rollingInterval": "Hour",
          "fileSizeLimitBytes": "",
          "retainedFileCountLimit": ""
        }
      }
    ]
  }
}
